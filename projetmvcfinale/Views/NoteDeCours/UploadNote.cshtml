@model projetmvcfinale.Models.NoteDeCours

<h4>Ajouter un fichier de note de cours</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="UploadNote" enctype="multipart/form-data" method="post" asp-controller="NoteDeCours" asp-action="UploadNote">
            <div class="form-group">
                <label asp-for="Lien" class="control-label"></label>
                <input asp-for="Lien" class="form-control" type="file" />
                <span asp-validation-for="Lien" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Téléverser" class="btn btn-info" />
            </div>
        </form>
    </div>
</div>
<div>
    <a asp-action="ListeNoteDeCours" class="btn btn-primary">Retour à la liste</a>
</div>

