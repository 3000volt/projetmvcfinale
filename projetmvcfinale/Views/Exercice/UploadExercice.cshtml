@model projetmvcfinale.Models.Exercice

<h4>Ajouter un fchier</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="UploadExercice" asp-controller="Exercice" enctype="multipart/form-data" method="post">
            <div class="form-group">
                <label asp-for="Lien" class="control-label"></label>
                <input asp-for="Lien" class="form-control" />
                <span asp-validation-for="Lien" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Téléverser" class="btn btn-info" />
            </div>
        </form>
    </div>
</div>
<div>
    <a asp-action="Index" class="btn btn-primary">Retour à la liste</a>
</div>

